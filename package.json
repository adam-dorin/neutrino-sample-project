{
  "name": "spanjs-sample-project",
  "version": "0.0.2",
  "description": "Test env for spanjs",
  "main": "index.js",
  "scripts": {
    "webpack-dev-server": "webpack-dev-server --config webpack.config.js",
    "start": "webpack-dev-server",
    "clean": "rm dist/bundle.js",
    "build-dev": "webpack --config webpack.config.js -d --env.development --mode development",
    "build-prod": " webpack --config webpack.config.js -p --env.production --mode production",
    "analize-prod": "webpack --config webpack-analysis.config.js -p --env.production --mode production",
    "analize": "webpack --profile --json > stats.json",
    "start-php": "php -S localhost:9000",
    "gzip-e": "gzip -9 -c public/main.bundle.js public/runtime.bundle.js | wc -c | numfmt --to=iec-i --suffix=B --padding=10",
    "cat-e": "cat public/main.bundle.js public/runtime.bundle.js | wc -c | numfmt --to=iec-i --suffix=B --padding=10",
    "move-resources": "node ./resource-parser.js",
    "compare": "npm run build-prod && npm run cat-e && npm run gzip-e"
  },
  "author": "Dorin Adam",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-loader": "^8.0.6",
    "cheerio": "^1.0.0-rc.3",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "html-webpack-template": "^6.2.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^7.0.0",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.32.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.4.1"
  },
  "dependencies": {}
}
